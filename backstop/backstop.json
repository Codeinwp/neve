{
  "id": "neve-starter-sites",
  "onReadyScript": "show-hidden.js",
  "viewports": [
    {
      "label": "Mobile",
      "width": 320,
      "height": 640
    },
    {
      "label": "Tablet",
      "width": 1024,
      "height": 768
    },
    {
      "label": "Desktop",
      "width": 1920,
      "height": 1080
    }
  ],
  "scenarios": [
    {
      "label": "neve-main-gutenberg",
      "selectors": [
        "document"
      ],
      "scrollToSelector": "footer",
      "delay": 1500,
      "hideSelectors": [
        ".wp-block-embed-youtube"
      ],
      "url": "http://localhost:8055"
    },
    {
      "label": "neve-main",
      "selectors": [
