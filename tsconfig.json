{
  "compilerOptions": {
    "baseUrl": "cypress",
    "target": "es5",
    "downlevelIteration": true,
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "cypress/**/**/*.ts",
    "cypress/**/**/**/*.ts",
    "cypress/support/index.ts",
    "cypress/**/*.ts",
    "cypress/plugins/index.js"
  ]
}