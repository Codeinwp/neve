{
  "root": true,
  "extends": [
	"plugin:@wordpress/eslint-plugin/recommended",
	"plugin:@typescript-eslint/recommended",
	"plugin:import/typescript"
  ],
  "rules": {
	"@typescript-eslint/ban-ts-comment": "off"
  },
  "ignorePatterns": [
	"**/vendor/**"
  ],
  "settings": {
	"import/parsers": {
	  "@typescript-eslint/parser": [
		".ts",
		".tsx"
	  ]
	},
	"import/resolver": {
	  "typescript": {
		"alwaysTryTypes": true
	  }
	}
  }
}
